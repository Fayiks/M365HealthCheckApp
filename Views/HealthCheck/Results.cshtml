@{
}

@model dynamic

@{
    ViewData["Title"] = "Health Check Results";
}

<h2>Health Check Results</h2>

<h4>Graph API Issues</h4>
<ul>
    @foreach (var issue in Model.GraphIssues)
    {
        <li>@issue</li>
    }
</ul>

<h4>Mailbox Issues</h4>
<ul>
    @foreach (var issue in Model.MailboxIssues)
    {
        <li>@issue</li>
    }
</ul>

<a class="btn btn-secondary" href="/Directory">Back to Directory</a>
